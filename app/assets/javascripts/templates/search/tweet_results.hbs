{{partial 'search/search'}}
<div class="display_card">
  <div class="card_title">Tweets</div>
  {{#each tweet in tweets.matches}}
    {{render "stream.post_partial" tweet}}
  {{else}}
    <div class="card-section">
      No results.
    </div>
  {{/each}}
  {{#if tweets.more}}
    <div class="card-section">
      Search query returned too many results - please narrow the search.
    </div>
  {{/if}}
</div>
