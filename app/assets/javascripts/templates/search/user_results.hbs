{{partial 'search/search'}}
<div class="display_card">
  <div class="card_title">Users</div>
  {{#each user in users.matches}}
    {{render 'search.user_partial' user}}
  {{else}}
    <div class="card-section">
      No results.
    </div>
  {{/each}}
  {{#if users.more}}
    <div class="card-section">
      Search query returned too many results - please narrow the search.
    </div>
  {{/if}}
</div>
