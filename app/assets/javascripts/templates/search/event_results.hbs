{{partial 'search/search'}}
<div class="display_card">
  <div class="card_title">Events</div>
  {{#each event in model.events.matches}}
    {{render "schedule.meta_partial" event}}
  {{else}}
    <div class="card-section">
      No results.
    </div>
  {{/each}}
  {{#if model.events.more}}
    <div class="card-section">
      Search query returned too many results - please narrow the search.
    </div>
  {{/if}}
</div>
